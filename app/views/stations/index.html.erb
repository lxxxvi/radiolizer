<h1>Stations</h1>

<table>
  <tr>
      <th>Station</th>
      <th>Played songs</th>
  </tr>

  <% @stations.each do |station| %>
    <tr>
      <td><%= link_to station['station_name'], station_path( id: station['station_id'] ) %></td>
      <td><%= station['played_songs'] %></td>
    </tr>
  <% end %>


</table>


