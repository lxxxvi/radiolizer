<h1>Stations</h1>

<ul>
  <% @stations.each do |station| %>
    <li>
      <div class="station">
        <div>
            <div class="play-count"><%= station['played_songs'] %></div>
        </div>
        <div class="vertical-center">
          <div>
            <%= link_to station['station_name'], station_path( id: station['station_id'] ) %></td>
          </div>
        </div>

      </div>
    </li>
  <% end %>
</ul>


